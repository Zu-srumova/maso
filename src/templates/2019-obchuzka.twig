{% extends '_base.twig' %}

{% block js_head %}
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/gh/fancyapps/fancybox@3.5.6/dist/jquery.fancybox.min.js"></script>
{% endblock %}

{% block og %}
    <meta property="og:image" content="http://masopust.roztoc.cz/img/ilustracni/hromadko-holac_cay8nd_c_scale,w_890.jpg">
    <meta property="og:title" content="Galerie – Roztocký Masopust 2019">
    <meta property="og:description" content="Fotografie z roztockého Masopustu. Máte nějaké vlastní? Pošlete nám je!">
    <meta property="og:url" content="http://masopust.roztoc.cz/galerie/foto2019">
    {% endblock %}

{% block title %}Galerie 2019{% endblock %}

{% block nav %}
    {% include '__nav.twig' with {'active': 'galerie'} %}
{% endblock %}

{% block content %}

{% block text %}

<h1>Masopustní obchůzka 2019</h1>
<p>← <a href="galerie">Galerie</a></p>

<div class="row">
    {% for i in 1..20 %}
    {% set num %}{% if i < 10 %}0{% endif %}{{ i }}{% endset %}
    <div class="card maso-foto-card">
        <div class="card-body">
            <a data-fancybox="2019-obchuzka" href="/img/galerie/2019-obchuzka/2019-obchuzka{{ num }}.jpg" data-caption="Masopustní obchůzka 2019 (foto Marek Musil)">
                <img class="card-img-top" src="/img/galerie/2019-obchuzka/thumb/2019-obchuzka{{ num }}.jpg" alt="Fotka od Marka Musila z obchůzky, krásná a nepopsatelná.">
            </a>
        </div>
    </div>
    {% endfor %}

    {# Fotky Petry Mandové #}
    {% for i in 21..28 %}
    {% set num %}{% if i < 10 %}0{% endif %}{{ i }}{% endset %}
    <div class="card maso-foto-card">
        <div class="card-body">
            <a data-fancybox="2019-obchuzka" href="/img/galerie/2019-obchuzka/600/2019-obchuzka{{ num }}.jpg" data-srcset="/img/galerie/2019-obchuzka/2019-obchuzka{{ num }}.jpg 2048w, /img/galerie/2019-obchuzka/1800/2019-obchuzka{{ num }}.jpg 1800w, /img/galerie/2019-obchuzka/1200/2019-obchuzka{{ num }}.jpg 1200w, /img/galerie/2019-obchuzka/600/2019-obchuzka{{ num }}.jpg 600w, /img/galerie/2019-obchuzka/thumb/2019-obchuzka{{ num }}.jpg 320w" data-caption="Masopustní obchůzka 2019 (foto Petra Mandová)">
                <img class="card-img-top" src="/img/galerie/2019-obchuzka/thumb/2019-obchuzka{{ num }}.jpg" alt="Fotka od Petry Mandové z obchůzky, krásná a nepopsatelná.">
            </a>
        </div>
    </div>
    {% endfor %}
    
    {# Fotky Kuby Sedláka #}
    {% for i in 29..46 %}
    {% set num %}{% if i < 10 %}0{% endif %}{{ i }}{% endset %}
    <div class="card maso-foto-card">
        <div class="card-body">
            <a data-fancybox="2019-obchuzka" href="/img/galerie/2019-obchuzka/600/2019-obchuzka{{ num }}.jpg" data-srcset="/img/galerie/2019-obchuzka/2019-obchuzka{{ num }}.jpg 2048w, /img/galerie/2019-obchuzka/1800/2019-obchuzka{{ num }}.jpg 1800w, /img/galerie/2019-obchuzka/1200/2019-obchuzka{{ num }}.jpg 1200w, /img/galerie/2019-obchuzka/600/2019-obchuzka{{ num }}.jpg 600w, /img/galerie/2019-obchuzka/thumb/2019-obchuzka{{ num }}.jpg 320w" data-caption="Masopustní obchůzka 2019 (foto Jakub Sedlák)">
                <img class="card-img-top" src="/img/galerie/2019-obchuzka/thumb/2019-obchuzka{{ num }}.jpg" alt="Fotka od Jakuba Sedláka z obchůzky, krásná a nepopsatelná.">
            </a>
        </div>
    </div>
    {% endfor %}
</div>

{% endblock %}
{% endblock %}